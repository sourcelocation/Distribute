      "sql": [
        {
          "dialect": "sqlite",
          "sql": "CREATE TABLE IF NOT EXISTS \"songs\" (\"id\" TEXT NOT NULL, \"title\" TEXT NOT NULL, \"album_id\" TEXT NOT NULL REFERENCES albums (id), \"duration_seconds\" INTEGER NOT NULL, \"file_id\" TEXT NULL, \"format\" TEXT NULL, \"is_downloaded\" INTEGER NOT NULL DEFAULT 0 CHECK (\"is_downloaded\" IN (0, 1)), \"updated_at\" INTEGER NULL, \"deleted_at\" INTEGER NULL, PRIMARY KEY (\"id\"));"
